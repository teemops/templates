# {{ ansible_managed }}

server {
        listen 80 default_server;
        listen [::]:80 default_server ipv6only=on;
        server_name {{ server_name }};

        root {{ root }};
        index {{ index }};

        location / {
                try_files $uri $uri/ =404;
        }

        location /rc- {
                try_files $uri $uri/ =404;
        }
}
